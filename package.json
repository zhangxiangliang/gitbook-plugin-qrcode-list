{
  "name": "gitbook-plugin-qrcode-list",
  "version": "0.1.0",
  "description": "Gitbook page footer show qrcode list",
  "main": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pushmetop/gitbook-plugin-qrcode-list.git"
  },
  "scripts": {
    "test": "istanbul cover -x 'lib/**' node_modules/mocha/bin/_mocha && codecov -F istanbul_mocha"
  },
  "keywords": [
    "pushmetop",
    "gitbook-plugin",
    "footer",
    "qrcode"
  ],
  "author": "pushmetop <admin@pushme.top> (http://github.com/pushmetop)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pushmetop/gitbook-plugin-qrcode-list/issues"
  },
  "homepage": "https://github.com/pushmetop/gitbook-plugin-qrcode-list#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0"
  },
  "engines": {
    "gitbook": "*"
  },
  "gitbook": {
    "properties": {
      "title": {
        "type": "string",
        "default": "",
        "description": "this block title"
      },
      "description": {
        "type": "string",
        "default": "",
        "description": "this block description"
      },
      "only": {
        "type": "array",
        "default": "[]",
        "description": "if in only show qrcode list"
      },
      "except": {
        "type": "array",
        "default": "[]",
        "description": "if not in except show qrcode list"
      },
      "lists": {
        "type": "array",
        "default": "[]",
        "description": "item is {'src': '', 'content': '', 'alt':'', 'text': '', 'margin': 4}"
      },
      "src": {
        "type": "string",
        "default": "''",
        "description": "qrcode img src"
      },
      "content": {
        "type": "string",
        "default": "''",
        "description": "qrcode description"
      },
      "alt": {
        "type": "string",
        "default": "''",
        "description": "qrcode img alt"
      },
      "text": {
        "type": "string",
        "default": "''",
        "description": "if qrcode img src is null, will use text to qrcode img"
      },
      "margin": {
        "type": "number",
        "default": "''",
        "description": "set text generate qrcode margin"
      }
    }
  }
}
